Xft.antialias:	1
Xft.hinting:	1
Xft.autohint:	0
Xft.hintstyle:	hintslight
Xft.rgba:	rgb
Xft.lcdfilter:	lcddefault

st.font:         JetBrainsMono Nerd Font:style:normal:pixelsize=15

! window padding
st.borderpx: 30

!- 0.1 to 0.9 = transparency
st.alpha: 1.0

*background: #161616
*foreground: #ffffff

! Black + DarkGrey
*color0:  #161616
*color8:  #262626

! DarkRed + Red
*color1:  #ee5396
*color9:  #ee5396

! DarkGreen + Green
*color2:  #42be65
*color10: #42be65

! DarkYellow + Yellow
*color3:  #be95ff
*color11: #be95ff

! DarkBlue + Blue
*color4:  #33b1ff
*color12: #33b1ff

! DarkMagenta + Magenta
*color5:  #ff7eb6
*color13: #ff7eb6

! DarkCyan + Cyan
*color6:  #08bdba
*color14: #08bdba

! LightGrey + White
*color7:  #f2f4f8
*color15: #ffffff
